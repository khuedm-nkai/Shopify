  <script type="text/javascript">
      const btnPreviewOnScroll = () => {
        window.onscroll = function () {
          var stickyPreviewBtn = document.querySelector(
            ".pfh-customily-preview-button"
          );
          var teeFormAction = document.querySelector("#teeFormActions");
          var btnPreview =
            teeFormAction.children[1].id === "tee-btn--preview"
              ? teeFormAction.children[1]
              : teeFormAction.children[2];
          var stickyButton = document.querySelector(".pfh-clone-preview-btn");
          var btnPreviewPosition = btnPreview?.getBoundingClientRect();
      
          if (btnPreviewPosition) {
            if (
              btnPreviewPosition.top <= window.innerHeight &&
              btnPreviewPosition.bottom >= 50
            ) {
              stickyButton.style.opacity = "0";
              stickyButton.parentElement.style.background = "none";
            } else {
              stickyButton.style.opacity = "1";
              stickyButton.parentElement.style.backgroundColor = "#fff";
              stickyButton.style.setProperty(
                "-webkit-text-fill-color",
                "#fff",
                "important"
              );
              stickyButton.style.setProperty("border-color", "#f4993d ", "important");
            }
          }
        };
      };
      
      const createPreviewBtn = () => {
        var btnPreview = `<div class="show-mobile-only-sticky-button pfh-customily-preview-button"> <button type="button" class="customily-preview-button ld-over-inverse pfh-clone-preview-btn" data-label="Add to cart" id="customily-preview-button"> <div class="ld ld-cycle ld-ring"></div>Preview</button></div>`;
        document.querySelector(".product .product__info-wrapper").insertAdjacentHTML("beforebegin", btnPreview);
      };
      
      const triggerClickPreviewBtn = () => {
        var stickyPreviewBtn = document.querySelector(
          ".pfh-customily-preview-button"
        );
        var previewBtn = document.querySelector(".tee-form-action .tee-btn--preview");
        if (stickyPreviewBtn && previewBtn) {
          stickyPreviewBtn.addEventListener("click", () => {
            var confirmedInput = document.querySelector('input[name="confirmed"]');
            if (confirmedInput) {
              previewBtn.click();
              if (!confirmedInput.checked) {
                confirmedInput.scrollIntoView({ behavior: "smooth", block: "start" });
              } else {
              }
            } else {
              previewBtn.click();
            }
          });
        }
      };
      
      document.addEventListener("teeinblue-event-component-injected", () => {
        createPreviewBtn();
        btnPreviewOnScroll();
        triggerClickPreviewBtn();
      });
  </script>
  
