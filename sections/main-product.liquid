  <script>
        var product = {{ product | json }};
        let day1 = 3
        let day2 = 4
        let day3 = 12
        let day4 = 16
      
      if(product.vendor == 'Dreamship' || product.vendor == "Senprints"){
        day1 = 2
        day2 = 3
        day3 = 3
        day4 = 13
      }else{
        day1 = 3
        day2 = 4
        day3 = 12
        day4 = 16
      }
      
      var currentDate = new Date();
      var order_ships_1_date = new Date(currentDate);
      order_ships_1_date.setDate(currentDate.getDate() + day1);
      var deliver_1_date = new Date(currentDate);
      deliver_1_date.setDate(currentDate.getDate() + day3);
      var order_ships_2_date = new Date(currentDate);
      order_ships_2_date.setDate(currentDate.getDate() + day2);
      var deliver_2_date = new Date(currentDate);
      deliver_2_date.setDate(currentDate.getDate() + day4);
      
      var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
      
      var month_order_ships_1 = monthNames[order_ships_1_date.getMonth()];
      var day_order_ships_1 = order_ships_1_date.getDate();
      var month_deliver_1 = monthNames[deliver_1_date.getMonth()];
      var day_deliver_1 = deliver_1_date.getDate();
  
      var month_order_ships_2 = monthNames[order_ships_2_date.getMonth()];
      var day_order_ships_2 = order_ships_2_date.getDate();
      var month_deliver_2 = monthNames[deliver_2_date.getMonth()];
      var day_deliver_2 = deliver_2_date.getDate();
  
      var month = monthNames[currentDate.getMonth()];
      var day = currentDate.getDate();
      let order_placed = month + " " + ("0" + day).slice(-2);
  
      var timelineCurrentElements = document.querySelectorAll('.timeline-current');
        timelineCurrentElements.forEach(function(element) {
        element.innerText = order_placed;
      });
  
      var order_ships = month_order_ships_1 + " " + day_order_ships_1 + "-" + day_order_ships_2;
      var delivered = month_deliver_1 + " " + day_deliver_1 + "-" + day_deliver_2;
      document.querySelector('.timeline-ship').textContent = order_ships;
      document.querySelector('.timeline-delivered').textContent = delivered;
      document.getElementById('delivered').textContent = delivered;
      document.getElementById('delivered-1').textContent = delivered;
  </script>
  
