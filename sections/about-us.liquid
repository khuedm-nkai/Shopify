{% style %}
  .about-us-container {
        max-width: 1280px;
        margin-left: auto;
        margin-right: auto;
        display: block;
    }

    .about-us-container p {
        font-size: 16px;
        font-family: Poppins, sans-serif;
    }

    .one-whole {
        width: calc(100% - 20px);
    }

    .colunn {
        margin-left: 10px;
        margin-right: 10px;
    }

    .rte {
        word-break: break-word;
    }

    .mobile_title {
        font-size: 20px !important;
        font-family: Nunito, sans-serif;
        font-weight: 700 !important;
        line-height: 28px !important;
    }

    .text-align-center {
        text-align: center;
    }

    .heading-content {
        font-family: Nunito, sans-serif;
        font-weight: 700;
        font-style: normal;
        text-transform: none;
        line-height: 1.5;
        color: #333;
        display: block;
        letter-spacing: 0px;
        text-rendering: optimizeLegibility;
        font-size: 35px;
        margin: 0 auto 15px 0;
    }

    .desktop_secondary_button {
        font-size: 28px !important;
        line-height: 40px !important;
        font-weight: 700 !important;
    }

    .custom-ul {
        list-style-image: initial;
        list-style: disc outside;
        margin-bottom: 20px;
    }

    .custom-ul .custom-li {
        margin-bottom: 12px;
    }

    .heading-content ul li{
        font-family: Nunito, sans-serif;
    }
{% endstyle %}
