{% style %}
.payment-method-wrapper {
  max-width: 1280px !important;
  margin-top: 96px;
  margin-left: auto;
  margin-right: auto;
}

div.content {
  padding: 30px 0 15px;
}

.dino-container {
  position: relative;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
}

/* Whole */
.dino-one-whole {
  width: calc(100% - 20px);
}

.rte {
  word-break: break-word;
}

h2,
.h2,
h2.title {
  font-family: Nunito, sans-serif;
  font-weight: 700;
  font-style: normal;
  text-transform: none;

  line-height: 48px;
  color: #0c6574;
  display: block;
  letter-spacing: 0px;
  text-rendering: optimizeLegibility;

  font-size: 40px;
  margin-bottom: 0.75em;
}

h1 {
  font-family: Nunito, sans-serif;
  font-style: normal;
  text-transform: none;
  line-height: 1.5;
  color: #333;
  display: block;
  letter-spacing: 0px;
  text-rendering: optimizeLegibility;
  font-size: 44px;
  margin: 0 auto 15px;
  clear: both;
  font-weight: 700;
  padding-top: 4px;
}

a,
a:visited,
a span {
  color: #df6518;
  text-decoration: none;
  position: relative;
  transition: color 0.1s linear;
}

a {
  touch-action: manipulation;
}

b,
strong {
  font-weight: bold;
}
{% endstyle %}
