    "custom_liquid_mG9GCP": {
      "type": "custom-liquid",
      "disabled": true,
      "settings": {
        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Responsive Image Grid<\/title>\n    <style>\n        .container_shop_product {\n            width: 100%;\n            max-width: 1100px;\n            margin: 0 auto;\n            text-align: center;\n            padding: 20px 30px;\n        }\n\n        .title_shop_product {\n            font-weight: bold;\n            font-size: 4rem;\n            margin-bottom: 20px;\n        }\n\n        .grid-container_shop_product {\n            display: grid;\n            gap: 20px;\n            grid-template-columns: repeat(3, 1fr);\n        }\n\n        @media (min-width: 768px) {\n            .grid-container_shop_product {\n                grid-template-columns: repeat(4, 1fr);\n            }\n        }\n\n        @media (min-width: 1024px) {\n            .grid-container_shop_product {\n                grid-template-columns: repeat(5, 1fr);\n            }\n        }\n\n        .image-container_shop_product {\n            text-align: center;\n            margin-bottom: 20px;\n        }\n\n        .image-wrapper {\n            position: relative;\n            width: 100%;\n            padding-top: 100%; \/* 1:1 Aspect Ratio *\/\n        }\n\n        .image_product {\n            position: absolute;\ncursor:pointer;\n            top: 0;\n            left: 0;\n            width: 100%;\n            height: 100%;\n            object-fit: cover;\n            border-radius: 50%;\n            transition: transform 0.3s ease-in-out;\n        }\n\n        .image_product:hover {\n            transform: scale(1.1);\n        }\n\n        .caption_product {\n            margin-top: 10px;\n            position: relative;\n            font-weight: 400;\n            font-size: 2rem;\n        }\n\n        .underline_product {\n            position: absolute;\n            left: 50%;\n            bottom: -4px;\n            height: 2px;\n            width: 0;\n            background-color: black;\n            transition: all 0.3s ease-in-out;\n        }\n\n        .caption_product:hover .underline_product {\n            width: 50%;\n            left: 25%;\n        }\n    <\/style>\n<\/head>\n<body>\n    <div class=\"container_shop_product\">\n        <div class=\"title_shop_product\">SHOP BY PRODUCT<\/div>\n        <div class=\"grid-container_shop_product\" id=\"grid-container_shop_product\">\n            <!-- Images will be added by JavaScript -->\n        <\/div>\n    <\/div>\n\n    <script>\n        function updateImagesToShow() {\n            const width = window.innerWidth;\n            const gridContainer = document.getElementById('grid-container_shop_product');\n            let imagesToShow;\n\n            if (width < 768) {\n                imagesToShow = 9;\n            } else if (width < 1024) {\n                imagesToShow = 8;\n            } else {\n                imagesToShow = 10;\n            }\n\n            \/\/ Remove existing images\n            while (gridContainer.firstChild) {\n                gridContainer.removeChild(gridContainer.firstChild);\n            }\n\n            \/\/ Add new images\n            for (let i = 0; i < imagesToShow; i++) {\n                const imageContainer = document.createElement('div');\n                imageContainer.classList.add('image-container_shop_product');\n\n                const imageWrapper = document.createElement('div');\n                imageWrapper.classList.add('image-wrapper');\n\n                const img = document.createElement('img');\n                img.src = 'https:\/\/wallpapercave.com\/wp\/wp9826189.jpg';\n                img.alt = `Image ${i + 1}`;\n                img.classList.add('image_product');\n\n                const caption = document.createElement('p');\n                caption.classList.add('caption_product');\n                caption.innerHTML = `Image ${i + 1}<span class=\"underline_product\"><\/span>`;\n\n                imageWrapper.appendChild(img);\n                imageContainer.appendChild(imageWrapper);\n                imageContainer.appendChild(caption);\n                gridContainer.appendChild(imageContainer);\n            }\n        }\n\n        window.addEventListener('resize', updateImagesToShow);\n        window.addEventListener('load', updateImagesToShow);\n    <\/script>\n<\/body>\n<\/html>",
