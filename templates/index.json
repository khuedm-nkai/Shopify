    "custom_liquid_MBjmg7": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Navbar<\/title>\n    <style>\n        .navbar {\nmargin:0 auto;\n            background-color: white;\n            overflow: hidden;\n            padding: 10px 0;\nmax-width:1100px;\n        }\n\n        .navbar-list {\n            list-style-type: none;\n            margin: 0;\n            padding: 0;\n            display: flex;\n            justify-content: space-around;\n            gap: 10px; \/* Khoảng cách giữa các nút *\/\n        }\n\n        .navbar-item {\n            flex-grow: 1;\n        }\n\n        .navbar-link {\n            display: block;\n            text-align: center;\n            padding: 14px 20px;\n            text-decoration: none;\n            color: white;\n            background-color: #bda98e; \/* Màu xám cho nút *\/\n            border-radius: 8px;\n            transition: background-color 0.3s, color 0.3s;\n        }\n\n        .navbar-link:hover {\n            background-color: #575757;\n            color: white;\n        }\n    <\/style>\n<\/head>\n<body>\n    <nav class=\"navbar\">\n        <ul class=\"navbar-list\">\n            {% assign menu_items = \"Home,About,Services,Portfolio,Contact,Blog\" | split: \",\" %}\n            {% for item in menu_items %}\n                <li class=\"navbar-item\"><a href=\"#\" class=\"navbar-link\">{{ item }}<\/a><\/li>\n            {% endfor %}\n        <\/ul>\n    <\/nav>\n<\/body>\n<\/html>",
