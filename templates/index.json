    "custom_liquid_y8DXbc": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Converted HTML CSS<\/title>\n    <style>\n        .banner_container {\n            display: flex;\n            width: 1200px;\n            height: 400px;\n            margin: auto;\n            gap: 20px;\n            align-items: center;\n            padding: 0 auto;\n        }\n\n        .main-banner {\n            width: 800px;\n            height: 400px;\n            background-color: yellow;\n            border-radius: 15px;\n            overflow: hidden;\n        }\n\n        .main-banner img {\n            width: 100%;\n            height: 100%;\n            object-fit: cover;\n        }\n\n        .side-banners {\n            display: flex;\n            flex-direction: column;\n            width: 400px;\n            height: 400px;\n            gap: 20px;\n        }\n\n        .side-banner {\n            width: 100%;\n            height: 200px;\n            border-radius: 15px;\n            overflow: hidden;\n        }\n\n        .side-banner img {\n            width: 100%;\n            height: 100%;\n            object-fit: cover;\n        }\n\n        .green-banner {\n            background-color: green;\n        }\n\n        .orange-banner {\n            background-color: orange;\n        }\n    <\/style>\n<\/head>\n<body>\n\n<div class=\"banner_container\">\n    <div class=\"main-banner\">\n        <img src=\"https:\/\/pawfecthouse.com\/cdn\/shop\/files\/MAIN-FTD-Banner-1-5x_5000x.progressive.png.jpg?v=1715700854\" alt=\"\">\n    <\/div>\n    <div class=\"side-banners\">\n        <div class=\"side-banner green-banner\">\n            <img src=\"https:\/\/pawfecthouse.com\/cdn\/shop\/files\/Summer_2024_Mini_Banner_5000x.progressive.png.jpg?v=1715590052\" alt=\"\">\n        <\/div>\n        <div class=\"side-banner orange-banner\">\n            <img src=\"https:\/\/pawfecthouse.com\/cdn\/shop\/files\/0301_photo_gift_collection_41d7e169-c2d0-4e44-9b23-3a9c8a10908a_5000x.progressive.png.jpg?v=1704342824\" alt=\"\">\n        <\/div>\n    <\/div>\n<\/div>\n\n<\/body>\n<\/html>",
        "color_scheme": "",
        "padding_top": 40,
        "padding_bottom": 52
      }
    },
